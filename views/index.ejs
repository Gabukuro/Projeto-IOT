<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <%- include('imports'); -%>
  </head>
  <body>
    <%- include('navbar'); -%>
    <div class="container">
      <h1><%= title %></h1>
      <p>Welcome to <%= title %></p>
      <div class="row">
        <div class="col-6">

            <% tempData.forEach((tempData) => { %>
             <%- include('tempSensorCard', {tempData}); -%>
              
            <% }) %>

        </div>

        <div class="col-6">

            <% actuators.forEach((actuator) => { %>
              <%- include('deviceCard', {actuator}); -%>
            <% }) %>
        </div>
        
      </div>
    </div>
  </body>
</html>
